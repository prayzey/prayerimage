{
    "version": 2,
    "functions": {
        "api/index.py": {
            "memory": 1024,
            "maxDuration": 60
        }
    },
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        },
        {
            "src": "templates/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "/static/favicon.ico"
        },
        {
            "src": "/",
            "dest": "/api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ]
}
